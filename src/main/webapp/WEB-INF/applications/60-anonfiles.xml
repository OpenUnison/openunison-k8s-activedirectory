<application name="anonfiles" azTimeoutMillis="30000" >
  <urls>
    <url regex="false" authChain="anon" overrideHost="true" overrideReferer="true">
      <host>#[OU_HOST]</host>
      <filterChain>

      </filterChain>
      <uri>/favicon.ico</uri>
      <results>
        <azSuccess>
        </azSuccess>
      </results>
      <azRules>
        <rule scope="dn" constraint="o=Tremolo" />
      </azRules>
    </url>

  </urls>
  <cookieConfig>
    <sessionCookieName>anon</sessionCookieName>
    <domain>*</domain>
    <logoutURI>/favicon.ico</logoutURI>
    <keyAlias>session-unison</keyAlias>
    <secure>false</secure>
    <timeout>true</timeout>
    <scope>-1</scope>
  </cookieConfig>
</application>
